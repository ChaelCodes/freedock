<h1>Stations (<%= @stations.length %>)</h1>
<h2>Presets</h2>
<ul>
  <% DivvyApi::PRESETS.keys.each do |preset| %>
    <li><%= link_to preset, station_path(preset) %></li>
  <% end %>
</ul>
<h2>Stations</h2>
<ul>
  <% @stations.sort_by{|station| station["name"]}.each do |station| %>
    <li><%= link_to "#{station["name"]} (#{station["station_id"]})", station_path(station["station_id"])  %></li>
<% end %>
</ul>
